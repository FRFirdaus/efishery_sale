<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="efishery_static_token_config_settings" model="ir.ui.view">
        <field name="name">static_token_settings</field>
        <field name="model">res.config.settings</field>
        <field name="inherit_id" ref="base.res_config_settings_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//div[hasclass('settings')]" position="inside">
                <div class="app_settings_block" data-string="Efishery" string="Efishery" data-key="efishery_sale">
                    <h2>Efishery Static Token</h2>
                    <div class="row mt16 o_settings_container">
                        <div class="col-xs-12 col-md-12 o_setting_box">
                            <div class="o_setting_right_pane">
                                <div class="content-group">
                                    <div class="mt16 row">
                                        <label for="efishery_static_token" string="Static Token" class="col-xs-3 col-md-3 o_light_label"/>
                                        <field name="efishery_static_token"/>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </xpath>
        </field>
    </record>
</odoo>